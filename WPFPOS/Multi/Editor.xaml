<Window x:Class="Multi.Editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Multi"
        mc:Ignorable="d"
        Title="Editor" Height="450" Width="800">
	<UniformGrid Columns="2">
		<Label Content="Naziv:" FontSize="42"/>
		<TextBox Text="{Binding Naziv}" FontSize="42"/>
		<Label Content="Cena:" FontSize="42"/>
		<TextBox Text="{Binding Cena}" FontSize="42"/>
	</UniformGrid>
</Window>
